<dashboard version="1.1">
<label>FireMon Services</label>

<row>
<panel>
<html>
<![CDATA[
<div style="display:flex; justify-content: space-between; align-items:center;">
<h2>Services List</h2>
<img src="/static/appLogo.png" style="height:40px;" alt="FireMon Logo"/>
</div>
]]>
</html>
</panel>
</row>

<row>
<panel>
<table>
<search>
<query>| firemon_services</query>
<earliest>-24h@h</earliest>
<latest>now</latest>
</search>
<fieldColumn name="Name"/>
<fieldColumn name="Description"/>
<fieldColumn name="Vulnerability"/>
<fieldColumn name="Default_Risk"/>
<fieldColumn name="Protocol"/>
<fieldColumn name="Protocol_Number"/>
<fieldColumn name="Port"/>
<fieldColumn name="Port_End"/>
<fieldColumn name="ICMP_Type"/>
<fieldColumn name="ICMP_Code"/>
<fieldColumn name="Action"/>
</table>
</panel>
</row>
</dashboard>
